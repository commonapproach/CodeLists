    <rdfs:isDefinedBy rdf:resource="https://codelist.commonapproach.org/codeLists/PopulationServed#"/>
    <cids:hasName xml:lang="en">Official Languages Minority Communities</cids:hasName>
    <rdfs:label xml:lang="en">Official Languages Minority Communities</rdfs:label>
    <skos:definition xml:lang="en">In Canada, official-language minority communities are mainly French-speaking people living outside the province of Quebec and English-speaking people living in the province of Quebec.</skos:definition>
    <cids:hasDefinition xml:lang="en">In Canada, official-language minority communities are mainly French-speaking people living outside the province of Quebec and English-speaking people living in the province of Quebec.</cids:hasDefinition>
    <cids:definedBy rdf:resource="https://ontology.commonapproach.org/cids#ac"/>
    <cids:hasIdentifier>ps13</cids:hasIdentifier>
  </rdf:Description>



  <rdf:Description rdf:about="#ps14">
    <rdf:type rdf:resource="https://ontology.commonapproach.org/cids#PopulationServed"/>
    <rdf:type rdf:resource="https://ontology.commonapproach.org/cids#CODE"/>
