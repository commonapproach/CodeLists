    <cids:hasName xml:lang="en">Value of Loans Disbursed</cids:hasName>
    <cids:hasDescription xml:lang="en">Value of loans disbursed by the organization during the reporting period.</cids:hasDescription>
    <skos:note xml:lang="en">Organizations should footnote all assumptions used.</skos:note>
    <skos:scopeNote xml:lang="en">This metric is intended to capture the total value disbursed as loans by the organization during the reporting period.</skos:scopeNote>
    <cids:definedBy rdf:resource="https://ontology.commonapproach.org/cids#giin"/>
    <org:hasIdentifier>PI5476</org:hasIdentifier>
