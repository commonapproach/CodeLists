    <cids:hasName xml:lang="en">Claims Submitted</cids:hasName>
    <cids:hasDescription xml:lang="en">Number of claims submitted to the organization during the reporting period.</cids:hasDescription>
    <skos:note xml:lang="en">Organizations should footnote all assumptions used.</skos:note>
    <skos:scopeNote xml:lang="en">This metric is intended to capture the total number of claims submitted to the organization during the reporting period, even those that were eventually rejected.</skos:scopeNote>
    <cids:definedBy rdf:resource="https://ontology.commonapproach.org/cids#giin"/>
