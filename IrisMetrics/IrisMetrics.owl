    <cids:hasName xml:lang="en">Students Provided Full Scholarship</cids:hasName>
    <cids:hasDescription xml:lang="en">Number of students receiving full scholarship during the reporting period.</cids:hasDescription>
    <skos:note xml:lang="en">Organizations should footnote the details on scholarship eligibility requirements, source of scholarship funding, and other relevant details.</skos:note>
    <skos:scopeNote xml:lang="en">Full scholarships allow a student to attend school without tuition or fee payment required of the student or his/her family.</skos:scopeNote>
    <cids:definedBy rdf:resource="https://ontology.commonapproach.org/cids#giin"/>
